@using SurveysApp.Models

@model List<Survey>

<h1>Surveys</h1>

<@if (Model.Count == 0)
{
            <p>No surveys available.</p>
}
else
{
            <ul>
        @foreach (var survey in Model)
        {
                            <li><a href="@Url.Action("Details", new { id = survey.Id })">@survey.Title</a></li>
        }
            </ul>
}
